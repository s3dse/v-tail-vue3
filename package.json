{
    "name": "@s3_dse/v-tail-vue3",
    "version": "0.1.13",
    "author": {
        "name": "Sebastian Doerl",
        "email": "sebastian.doerl@gmail.com"
    },
    "license": "MIT",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "main": "dist/v-tail.umd.js",
    "module": "dist/v-tail.es.js",
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "scripts": {
        "serve": "vite --port 8081",
        "build": "vite build",
        "preview": "vite preview --port 8081",
